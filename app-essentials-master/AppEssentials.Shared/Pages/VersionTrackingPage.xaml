<?xml version="1.0" encoding="utf-8" ?>
<ContentPage
    x:Class="AppEssentials.Shared.Pages.VersionTrackingPage"
    xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml">
    <ContentPage.Content>
        <StackLayout>
            <Label
                x:Name="LabelCurrent"
                HorizontalOptions="CenterAndExpand"
                TextColor="Black"
                VerticalOptions="Center" />
            <Label
                x:Name="LabelPast"
                HorizontalOptions="CenterAndExpand"
                TextColor="Black"
                VerticalOptions="Center" />
            <Label
                x:Name="LabelNew"
                HorizontalOptions="CenterAndExpand"
                TextColor="Black"
                VerticalOptions="Center" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>